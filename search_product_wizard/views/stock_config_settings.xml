<odoo>
    <!--Inherited view of Stock Config Settings -->
    <record id="stock_config_settings_ext_views" model="ir.ui.view">
        <field name="name">stock settings extended</field>
        <field name="model">stock.config.settings</field>
        <field name="inherit_id" ref="stock.view_stock_config_settings"/>
        <field name="arch" type="xml">
            <xpath expr="//form" position="inside">
                <group col="4" string="Set Product Attributes">
                    <field name="field1"/>
                    <field name="field2"/>
                    <field name="field3"/>
                    <field name="field4"/>
                    <field name="field5"/>
                </group>
            </xpath>
        </field>
    </record>
</odoo>
