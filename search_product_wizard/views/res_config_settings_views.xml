<odoo>
    <record id="res_config_settings_search_product" model="ir.ui.view">
        <field name="name">res_config_settings_search_product</field>
        <field name="model">res.config.settings</field>
        <field name="inherit_id" ref="stock.res_config_settings_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//div[@data-key='stock']" position="inside">
                <h2>Product Attributes</h2>
                <div class="row mt16 o_settings_container">
                    <div class="col-xs-12 col-md-6 o_setting_box">
                        <div class="o_setting_left_pane">
                            <group col="4">
                                <field name="field1" style="width: 150px"/>
                                <field name="field2" style="width: 150px"/>
                                <field name="field3" style="width: 150px"/>
                                <field name="field4" style="width: 150px"/>
                                <field name="field5" style="width: 150px"/>
                            </group>
                        </div>
                    </div>
                </div>
            </xpath>
        </field>
    </record>
</odoo>
